{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "importOrder": ["express",
    "^(?!express)\\w+$", 
    "<THIRD_PARTY_MODULES>",
    "^assets/(.*)$", 
    "^@trivago", 
    "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}